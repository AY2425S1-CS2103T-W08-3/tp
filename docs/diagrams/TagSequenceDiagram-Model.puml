@startuml
!include style.puml
skinparam ArrowFontStyle plain

box Model MODEL_COLOR_T1
participant ":Model" as Model MODEL_COLOR
participant ":AddressBook" as AddressBook MODEL_COLOR
participant ":FilteredPersons" as FilteredPersons MODEL_COLOR
end box

Model -> AddressBook : setPerson(target, updatedPerson)
activate AddressBook
AddressBook --> Model
deactivate AddressBook

Model -> FilteredPersons : setPredicate(predicate)
activate FilteredPersons
deactivate FilteredPersons

@enduml